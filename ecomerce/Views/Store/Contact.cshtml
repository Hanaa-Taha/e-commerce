        <h1>اتصل بنا</h1>
        <!--<p> <a href="home.html">home >></a> contact </p>-->

    </div>


    <!--content section starts-->

    <section class="contact">

        <div class="icons-container">
            <div class="icons">
                <i class="fas fa-map-marker-alt"></i>
                <h3>عنواننا</h3>
                <p>الزقازيق , مصر</p>
            </div>

            <div class="icons">
                <i class="fas fa-envelope"></i>
                <h3>بريدنا الإلكتروني</h3>
                <p>student1@gmail.com</p>
                <p>student2@gmail.com</p>
            </div>

            <div class="icons">
                <i class="fas fa-phone"></i>
                <h3>أرقام الهاتف</h3>
                <p>+123-456-7890</p>
                <p>+111-222-3333</p>
            </div>





        </div>

        <div class="row">
            <iframe class="map" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3434.6866001885855!2d31.480334514607055!3d30.586391399722885!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14f7f053ae7dd993%3A0x82a66871a800f213!2sFaculty%20of%20Engineering%2C%20Zagazig%20University!5e0!3m2!1sen!2seg!4v1637878397294!5m2!1sen!2seg" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
            <form action="">
                <h3>تواصل معنا</h3>
                <div class="inputBox">
                    <input type="text" placeholder="اكتب الاسم" class="box">
                    <input type="email" placeholder="اكتب البريد الإلكتروني" class="box">
                </div>
                <div class="inputBox">
                    <input type="number" placeholder="اكتب رقم الهاتف" class="box">
                    <input type="text" placeholder="عنوان الرسالة" class="box">
                </div>
                <textarea placeholder="ادخل رسالتك" cols="30" rows="10"></textarea>
                <input type="submit" value="إرسال" class="btn">
            </form>


        </div>

    </section>


    <!--content section ends-->
    <!-- footer section starts  -->

    <section class="footer">

        <div class="box-container">

            <div class="box">
                <h3>روابط سريعة</h3>
                <a href="/store"> <i class="fas fa-tractor"></i> الصفحة الرئيسية </a>
