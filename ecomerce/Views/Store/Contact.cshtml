    <!-- header section starts  -->

    <header>

        <div class="header-2">

            <a href="#" class="logo"> <img src="~/lib/Images/icons8-farm-64.png" /> Farmer House </a>

            <form action="" class="search-bar-container">
                <input type="search" id="search-bar" placeholder="search here...">
                <label for="search-bar" class="fas fa-search"></label>
            </form>

        </div>

        <div class="header-3">

            <div id="menu-bar" class="fas fa-bars"></div>

            <nav class="navbar">
                <a href="/store">الصفحة الرئيسية</a>
                <a href="/store/shop">المتجر</a>
                <a href="/store/features">المميزات</a>
